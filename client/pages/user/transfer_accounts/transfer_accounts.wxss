
@import "../mine/mine.wxss";

.mine_welfare{
   display: flex;
   justify-content: flex-start;
    align-items: center;
    width: 80%;

}

.account{
    width: 44px;
    height: 44px;
    border-radius: 5px;
    margin-right: 15px;
}
.personal_message{

    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    color: #4a4a4a;
    width: 200px;
}
.personal_name{

    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    -webkit-line-clamp: 1;
}
.account_all{
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding-right: 15px;
    width: 94%;
}
.weui-accounts{
    display: flex;
    align-items: center;
}

.all_right{
    padding-left: 5px;
}
.reset_go{
    width: 20px;
    height: 20px;

}

.password_image{

    color: #4a4a4a;
}



.personal_tel{
color: #ababab;
}

.transfer_accounts{

    display: flex;
    justify-content: space-between;
    align-items: center;
    width: 92%;
    text-align: center;
    margin: 0 auto;
    font-size: 16px;
    color: #8a8a8a;

}

.all_right{
    color: #fe9728;
}
.del_accounts{

    padding: 15px;
}

.weui-help{
    line-height: 30px;
    font-size:14px;
    display: flex;
    justify-content: center;

}

.jx_left{
    color: #fe9728;
    text-align: right;
    padding: 0;
}

.jx_right{
    color: #fe9728;
    text-align: left;
}
.orange{
    color:  #fe9728;
}


.reset_icon{
    width: 16px;
    height: 16px;
}

span.line{
    padding-left: 10px;
    padding-right: 10px;
    color: #bbb;
}

.font_small{

    color: #ababab;
    font-size: 14px;
}

button.kf_button {

    position:relative;
    display:block;
    margin-left:0;
    margin-right:0;
    padding-left:0;
    padding-right: 0;
    box-sizing:border-box;
    font-size:14px;
    text-align:center;
    text-decoration:none;
    line-height:30px;
    border-radius:0;
    -webkit-tap-highlight-color:transparent;
    overflow:hidden;
    color: #fe9728;
    background-color:transparent;

}
.kf_button::after{
    border: 0px;
}
